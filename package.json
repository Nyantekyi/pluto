{
  "name": "pluto-interpreter",
  "version": "0.1.0",
  "description": "A simple, browser-compatible visual programming tool for creating custom executables",
  "type": "module",
  "main": "./dist/pluto.node.js",
  "module": "./dist/pluto.esm.js",
  "browser": "./dist/pluto.browser.js",
  "bin": {
    "pluto": "./cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/pluto.esm.js",
      "require": "./dist/pluto.node.js",
      "browser": "./dist/pluto.browser.js"
    }
  },
  "scripts": {
    "build": "node build.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "node src/index.js"
  },
  "keywords": [
    "interpreter",
    "visual-programming",
    "workflow",
    "nuxt"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.19.0",
    "vitest": "^1.0.0"
  }
}
